u32 cond_test(u32 v0) {
entry:
    result = stackalloc u32
    cond   = cmp eq u32 v0, 0

    bcond u1 cond, on_true, on_false

on_true:
    store u32* result, u32 100
    branch exit

on_false:
    store u32* result, u32 0
    branch exit

exit:
    read_result = load u32, u32* result
    ret u32 read_result
}
