i32 main(u8* buffer) {
	i32 x = 0;
	
	for (i32 y = 0; y < 1337; y += 1) {
		x = y;
	}

    i32 index = 0;

    while (buffer[index]) {
        buffer[index] += 1u8;
        index += 1;
    }

    return x;
}

